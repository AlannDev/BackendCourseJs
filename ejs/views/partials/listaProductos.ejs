<h1>Lista de Productos</h1>

<% if(productos.length > 0) { %>
    <table>
        <th>
            <td>Nombre</td>
            <td>Precio</td>
            <td>Url Foto</td>
        </th>
        <% productos.forEach( prod => { %>
            <tr>
                <td><%= prod.nombre %></td>
                <td><%= prod.precio %></td>
                <td><%= prod.urlFoto %></td>
            </tr>
        <% }) %>
    </table>
<% }else{ %>
    <h2>No hay productos</h2>
<% } %>

<a href="../cargarProducto">
    <input onclick="" type="submit" value="Cargar Producto">
</a>